   "source": [
    "n=11\n",
    "def fibo(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    elif n==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return fibo(n-1)+fibo(n-2)\n",
    "    \n",
    "list = [fibo(i) for i in range(n+1)]\n",
    "\n",
    "print list\n"
   ]

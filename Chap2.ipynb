    "def pascal(n):\n",
    "    liste = [[1],[1,1]]\n",
    " \n",
    "    print liste[0]\n",
    "    print liste[1]\n",
    "  \n",
    "    for i in range(2,n):\n",
    "        liste.append([1])\n",
    "        for j in range(1, i):\n",
    "            liste[i].append(liste[i-1][j-1] + liste[i-1][j])\n",
    "        liste[i].append(1) \n",
    "        print liste[i]\n",
    " \n",
    "pascal(10)"
   ]
